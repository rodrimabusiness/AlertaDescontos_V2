{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "lib/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/**/*": {
      "maxDuration": 60
    },
    "lib/**/*": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/lib/(.*)",
      "dest": "/lib/$1"
    }
  ]
}
